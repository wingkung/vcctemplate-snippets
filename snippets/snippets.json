{
    "vcc list page": {
        "prefix": "vcc list page",
        "body": [
            "<div class=\"bg-light lter b-b wrapper-md\">", 
            "    <h1 class=\"m-n font-thin h3\">$1</h1>", 
            "</div>", "<div class=\"wrapper-md\" ng-controller=\"$2\">", 
            "    <div class=\"row\">", 
            "        <div class=\"col-md-8\">", 
            "            <div class=\"panel panel-default\">", 
            "                <div class=\"panel-body\">", 
            "                    <form class=\"form-inline form-validation\" name=\"condForm\">", 
            "                        <div class=\"form-group\">", 
            "                        </div>", 
            "                        <div class=\"form-group\">", 
            "                            <button class=\"btn btn-default\" ng-click=\"search(page.page)\"><i class=\"fa fa-search\"></i></button>", 
            "                        </div>", 
            "                    </form>", 
            "                </div>", 
            "            </div>", "", 
            "        </div>", 
            "        <div class=\"col-md-4\">", 
            "            <div class=\"panel panel-default\">", 
            "                <div class=\"panel-body\">", 
            "                    <form class=\"form-inline form-validation\">", 
            "                        <div class=\"form-group\">", 
            "                            <button class=\"btn btn-primary\" ng-disabled=\"condForm.$invalid\" ng-click=\"add()\">新增</button>", 
            "                        </div>", 
            "                    </form>", 
            "                </div>", 
            "            </div>", 
            "        </div>", 
            "    </div>", 
            "    <div class=\"panel panel-default\">", 
            "        <div class=\"panel-body\">", 
            "            <table class=\"table table-hover\">", 
            "                <thead>", 
            "                    <tr>", 
            "                        <th class=\"text-center\"></th>", 
            "                        <th class=\"text-center\"></th>", 
            "                    </tr>", 
            "                </thead>", 
            "                <tbody>", 
            "                    <tr class=\"text-center\" ng-repeat=\"item in items\">", 
            "                        <td></td>", 
            "                        <td>",
            "                           <div class=\"btn-group-sm\" uib-dropdown>",
            "                               <button class=\"btn\" uib-dropdown-toggle>操作<span class=\"caret\"></span></button>",
            "                               <ul uib-dropdown-menu>",
            "                                   <li><a href=\"\" ng-click=\"edit(item)\">修改</a></li>",
            "                                   <li><a href=\"\" confirm=\"remove(item)\">删除</a></li>",
            "                               </ul>",
            "                           </div>",
            "                        </td>",
            "                    </tr>", 
            "                </tbody>", 
            "            </table>", 
            "        </div>", 
            "        <div class=\"row\">", 
            "            <div class=\"col-sm-4 hidden-xs\">", 
            "            </div>", 
            "            <div class=\"col-sm-4 text-center\">", 
            "                <small class=\"text-muted inline m-t-sm m-b-sm\">总共{{page.count}}条,当前{{page.page}}/{{page.total}}页</small>", 
            "            </div>", 
            "            <div class=\"col-sm-4 text-right text-center-xs\">", 
            "                <ul class=\"pagination pagination-sm m-t-none m-b-none\">", 
            "                    <li><a href=\"\" ng-click=\"search(1)\"><i class=\"fa fa-angle-double-left\"></i></a></li>", 
            "                    <li><a href=\"\" ng-click=\"search(page.page - 1)\"><i class=\"fa fa-angle-left\"></i></a></li>", 
            "                    <li><a href=\"\" ng-click=\"search(page.page + 1)\"><i class=\"fa fa-angle-right\"></i></a></li>", 
            "                    <li><a href=\"\" ng-click=\"search(page.total)\"><i class=\"fa fa-angle-double-right\"></i></a></li>", 
            "                </ul>", 
            "            </div>", 
            "        </div>", 
            "    </div>", 
            "</div>"
        ],
        "description": "新查询列表页面"
    },
    "vcc wrapper": {
        "prefix": "vcc wrapper",
        "body": [
            "<div class=\"bg-light lter b-b wrapper-md\">",
            "    <h1 class=\"m-n font-thin h3\">$1</h1>",
            "</div>",
            "<div class=\"wrapper-md\" ng-controller=\"$2\">",
            "</div>"
        ],
        "description": "新页模板"
    },
    "vcc dialog": {
        "prefix": "vcc dialog",
        "body": [
            "<div class=\"wrapper-md\">", 
            "    <form name=\"form\" class=\"form-validation\" novalidate>", 
            "        <div class=\"modal-body\">", "", "        </div>", 
            "        <div class=\"modal-footer\">", 
            "            <button class=\"btn btn-success\" ng-disabled=\"form.$invalid\" ng-click=\"save()\"><i class=\"fa fa-save\"></i>&nbsp;保存</button>", 
            "            <button class=\"btn btn-default\" ng-click=\"cancel()\"><i class=\"fa fa-times\"></i>&nbsp;关闭</button>", 
            "        </div>", 
            "    </form>", 
            "</div>"
        ],
        "description": "对话框模板"
    },
    "vcc form": {
        "prefix": "vcc form",
        "body": [
            "<form name=\"$1\" class=\"form-validation\">",
            "</form>"
        ],
        "description": "表单模板"
    },
    "vcc form input": {
        "prefix": "vcc form input",
        "body": [
            "<div class=\"form-group col-sm-3\">",
            "   <label class=\"text-right\">$1</label>",
            "   <div><input class=\"form-control\" placeholder=\"\" ng-model=\"$2\"></div>",
            "</div>"
        ],
        "description": "表单输入框"
    },
    "vcc form select": {
        "prefix": "vcc form select",
        "body": [
            "<div class=\"form-group col-sm-3\">",
            "   <label class=\"text-right\">$1</label>",
            "   <div><select class=\"form-control\" ng-model=\"$2\" ng-options=\"item.id as item.name for item in $3\"><option>请选择</option></select></div>",
            "</div>"
        ],
        "description": "表单输入框"
    },
    "vcc form selectize single": {
        "prefix": "vcc form selectize single",
        "body": [
            "<div class=\"form-group col-sm-3\">",
            "   <label class=\"text-right\">$1</label>",
            "   <div>", 
            "       <select selectize=\"{labelField:'$2',valueField:'$3',searchField:'$3'}\" ng-options=\"item.id as item.name for item in $4\" ng-model=\"$5\">",
            "           <option>请选择</option>",
            "       </select>", 
            "   </div>",
            "</div>"
        ],
        "description": "表单搜索单选"
    },
    "vcc form selectize tags": {
        "prefix": "vcc form selectize tags",
        "body": [
            "<div class=\"form-group col-sm-3\">",
            "   <label class=\"text-right\">$1</label>",
            "   <div>", 
            "       <select multiple=\"multiple\" selectize=\"{labelField:'$2',valueField:'$3',searchField:'$3'}\" ng-options=\"item.id as item.name for item in $4\" ng-model=\"$5\">",
            "           <option>请选择</option>",
            "       </select>",  
            "   </div>",
            "</div>"
        ],
        "description": "表单标签多选"
    },
    "vcc form switch": {
        "prefix": "vcc form switch",
        "body": [
            "<div class=\"form-group col-sm-3\">",
            "   <label class=\"text-right\">$1</label>",
            "   <div>",
            "       <label class=\"i-switch bg-info m-t-xs m-r\">",
            "           <input type=\"checkbox\" ng-true-value=\"1\" ng-false-value=\"0\" checked>",
            "           <i></i>",
            "       </label>",
            "   </div>",
            "</div>"
        ],
        "description": "表单开关"
    },
    "vcc form checkboxs": {
        "prefix": "vcc form checkboxs",
        "body": [
            "<div class=\"form-group col-sm-3\">",
            "   <label class=\"text-right\">$1</label>",
            "   <div>",
            "       <label class=\"m-r-sm i-checks\" ng-repeat=\"data in $2\">",
            "           <input type=\"checkbox\" ng-model=\"data._id\"><i></i> {{data.name}}",
            "       </label>",
            "   </div>",
            "</div>"
        ],
        "description": "表单多选框" 
    },
    "vcc form inputgroup": {
        "prefix": "vcc form inputgroup",
        "body": [
            "<div class=\"form-group col-sm-3\">",
            "    <label class=\"text-right\">$1</label>",
            "    <div class=\"input-group\">",
            "        <input class=\"form-control\">",
            "        <span class=\"input-group-btn\">",
            "            <button type=\"button\" class=\"btn btn-default\">$2</button>",
            "        </span>",
            "    </div>",
            "</div>"
        ],
        "description": "表单带按钮输入框" 
    },
    "vcc dropdown": {
        "prefix": "vcc dropdown",
        "body": [
            "<div class=\"btn-group-sm\" uib-dropdown>",
            "    <button class=\"btn\" uib-dropdown-toggle>下拉<span class=\"caret\"></span></button>",
            "    <ul uib-dropdown-menu>",
            "        <li><a href=\"\">$1</a></li>",
            "        <li class=\"divider\"></li>",
            "        <li><a href=\"\">$2</a></li>",
            "    </ul>",
            "</div>"
        ],
        "description": "下拉按钮" 
    },
    "vcc pagination": {
        "prefix": "vcc pagination",
        "body": [
            "<div class=\"col-sm-4 hidden-xs\">",
            "</div>",
            "<div class=\"col-sm-4 text-center\">",
            "    <small class=\"text-muted inline m-t-sm m-b-sm\">总共{{page.count}}条,当前{{page.page}}/{{page.total}}页</small>",
            "</div>",
            "<div class=\"col-sm-4 text-right text-center-xs\">",
            "    <ul class=\"pagination pagination-sm m-t-none m-b-none\">",
            "        <li><a href=\"\" ng-click=\"search(1)\"><i class=\"fa fa-angle-double-left\"></i></a></li>",
            "        <li><a href=\"\" ng-click=\"search(page.page - 1)\"><i class=\"fa fa-angle-left\"></i></a></li>",
            "        <li><a href=\"\" ng-click=\"search(page.page + 1)\"><i class=\"fa fa-angle-right\"></i></a></li>",
            "        <li><a href=\"\" ng-click=\"search(page.total)\"><i class=\"fa fa-angle-double-right\"></i></a></li>",
            "    </ul>",
            "</div>"
        ],
        "description": "分页模板" 
    }
}